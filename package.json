{
  "name": "study-angular",
  "version": "0.0.0",
  "scripts": {
    "======serve======": "",
    "start:local:web": "ng serve --configuration local --project web --disable-host-check",
    "start:local:admin": "ng serve --configuration local --project admin  --port 4201",
    "start:prod:web": "ng serve --configuration production --project web",
    "start:prod:admin": "ng serve --configuration production --project admin  --port 4201",
    "======build======": "",
    "build:web:dev": "ng build --configuration local --project=web --aot",
    "build:admin:dev": "ng build --configuration local --project=admin --aot",
    "build:web:prod": "ng build --configuration production --project=web",
    "build:admin:prod": "ng build --configuration production --project=admin",
    "======deploy:hosting======": "",
    "deploy:dev:hosting:web": "firebase deploy --project=default --only=hosting:web",
    "deploy:dev:hosting:admin": "firebase deploy --project=default --only=hosting:admin",
    "deploy:prod:hosting:web": "firebase deploy --project=prod --only=hosting:web",
    "deploy:prod:hosting:admin": "firebase deploy --project=prod --only=hosting:admin",
    "======build:and:deploy:hosting======": "",
    "buildAndDeploy:dev:hosting:web": "npm run build:web:dev && npm run deploy:dev:hosting:web",
    "buildAndDeploy:dev:hosting:admin": "npm run build:admin:dev && npm run deploy:dev:hosting:admin",
    "buildAndDeploy:prod:hosting:web": "npm run build:web:prod && npm run deploy:prod:hosting:web",
    "buildAndDeploy:prod:hosting:admin": "npm run build:admin:prod && npm run deploy:prod:hosting:admin"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@datorama/akita": "^8.0.1",
    "@tinymce/tinymce-angular": "^7.0.0",
    "algoliasearch": "^4.17.0",
    "file-saver": "^2.0.5",
    "firebase": "^9.21.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.29.4",
    "object-resolve-path": "^1.1.1",
    "rxjs": "~7.8.0",
    "swiper": "^9.2.4",
    "tinymce": "^6.4.2",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.7",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@types/algoliasearch": "^4.0.0",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "~4.3.0",
    "dn-schematics": "^0.0.13",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}